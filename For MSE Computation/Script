for k=1:2
	for i=1:57
		for j=12:34
			BT20(i,j)=rand(1)
		end
	end
	%headerRow = ['Time1' '' 'Time2' '' 'Time3\n'];
        headerRowbis=cellstr(headerRow);
    	filename=sprintf('test%d.csv', k);
     	%csvwrite(filename,headerRowbis);
    	fid = fopen(filename, 'w');
    	fprintf(fid,headerRow);
    	fclose(fid);
    	%csvwrite(filename,Time2);
    	dlmwrite(filename,BT20, '-append', 'precision', '%.4f', 'delimiter', ',');
end
